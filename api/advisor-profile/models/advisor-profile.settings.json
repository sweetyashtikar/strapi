{
  "kind": "collectionType",
  "collectionName": "advisor_profiles",
  "info": {
    "name": "advisorProfile",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "user": {
      "via": "advisor_profile",
      "plugin": "users-permissions",
      "model": "user"
    },
    "firstName": {
      "type": "string"
    },
    "lastName": {
      "type": "string"
    },
    "title": {
      "type": "string"
    },
    "professionalBackground": {
      "type": "richtext"
    },
    "topics": {
      "type": "richtext"
    },
    "otherAreas": {
      "type": "richtext"
    },
    "sessionsCompleted": {
      "type": "integer"
    },
    "founderConnections": {
      "type": "integer"
    },
    "hoursOffered": {
      "type": "integer"
    },
    "hoursConsumed": {
      "type": "decimal"
    },
    "profilePicture": {
      "model": "file",
      "via": "related",
      "allowedTypes": [
        "images"
      ],
      "plugin": "upload",
      "required": false,
      "pluginOptions": {}
    },
    "slug": {
      "type": "string"
    },
    "city": {
      "model": "city"
    },
    "accessToken": {
      "type": "string"
    },
    "refreshToken": {
      "type": "string"
    },
    "bookingUrl": {
      "type": "string"
    },
    "website": {
      "type": "string"
    },
    "companyName": {
      "type": "string"
    }
  }
}
