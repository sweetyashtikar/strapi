{
  "kind": "collectionType",
  "collectionName": "users-permissions_user",
  "info": {
    "name": "user",
    "description": ""
  },
  "options": {
    "privateAttributes": [
      "username"
    ],
    "draftAndPublish": false,
    "timestamps": true
  },
  "attributes": {
    "username": {
      "type": "string",
      "minLength": 3,
      "unique": true,
      "configurable": false,
      "required": true
    },
    "email": {
      "type": "email",
      "minLength": 6,
      "configurable": false,
      "required": true
    },
    "provider": {
      "type": "string",
      "configurable": false
    },
    "password": {
      "type": "password",
      "minLength": 6,
      "configurable": false,
      "private": true
    },
    "resetPasswordToken": {
      "type": "string",
      "configurable": false,
      "private": true
    },
    "confirmationToken": {
      "type": "string",
      "configurable": false,
      "private": true
    },
    "confirmed": {
      "type": "boolean",
      "default": false,
      "configurable": false
    },
    "blocked": {
      "type": "boolean",
      "default": false,
      "configurable": false
    },
    "role": {
      "model": "role",
      "via": "users",
      "plugin": "users-permissions",
      "configurable": false
    },
    "phone": {
      "type": "string",
      "required": false,
      "unique": false,
      "private": true
    },
    "phoneToken": {
      "type": "integer",
      "private": true
    },
    "emailToken": {
      "type": "integer",
      "private": true
    },
    "onboarded": {
      "type": "boolean",
      "default": false
    },
    "onboardedAt": {
      "type": "datetime",
      "private": true
    },
    "profile": {
      "via": "user",
      "model": "profile"
    },
    "lastLogin": {
      "type": "datetime"
    },
    "subscription": {
      "via": "user",
      "model": "subscription"
    },
    "badges": {
      "type": "json"
    },
    "advisor_profile": {
      "via": "user",
      "model": "advisor-profile"
    },
    "feed_posts": {
      "via": "user",
      "collection": "feed-post"
    },
    "feed_post_comments": {
      "via": "user",
      "collection": "feed-post-comment"
    },
    "liked_posts": {
      "via": "liked_by_users",
      "collection": "feed-post"
    },
    "notificationsSent": {
      "via": "userSender",
      "collection": "notification"
    },
    "notificationsReceived": {
      "via": "userReceiver",
      "collection": "notification"
    }
  }
}
